{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/LauraJouLee/IS545-Final-Project/main/elderly_population_US-state_id.csv"
  },
  "hconcat": [
    {
      "transform": [
        {
          "lookup": "id",
          "from": {
            "data": {
              "url": "data/us-10m.json",
              "format": {"type": "topojson", "feature": "states"}
            },
            "key": "id"
          },
          "as": "geo"
        }
      ],
      "projection": {"type": "albersUsa"},
      "mark": "geoshape",
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {"field": "total", "type": "quantitative"},
        "row": {"field": "year"}
      }
    },
    {
      "transform": [
        {
          "lookup": "id",
          "from": {
            "data": {
              "url": "data/us-10m.json",
              "format": {"type": "topojson", "feature": "states"}
            },
            "key": "id"
          },
          "as": "geo"
        }
      ],
      "projection": {"type": "albersUsa"},
      "mark": "geoshape",
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {"field": "female", "type": "quantitative"},
        "row": {"field": "year"}
      }
    },
    {
      "transform": [
        {
          "lookup": "id",
          "from": {
            "data": {
              "url": "data/us-10m.json",
              "format": {"type": "topojson", "feature": "states"}
            },
            "key": "id"
          },
          "as": "geo"
        }
      ],
      "projection": {"type": "albersUsa"},
      "mark": "geoshape",
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {"field": "male", "type": "quantitative"},
        "row": {"field": "year"}
      }
    }
  ],
  "config": {}
}